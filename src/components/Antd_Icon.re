let iconToJsSafe = (~icon=?, ()) =>
  icon
  |> (
    fun
    | Some(x) => iconToJs(x)
    | None => ""
  );

module AntIcon = {
  [@bs.module "antd"] [@react.component]
  external make:
    (
      ~_type: string,
      ~style: 'a,
      ~theme: 'b,
      ~spin: 'c,
      ~rotate: 'd,
      ~component: 'e,
      ~twoToneColor: 'f
    ) =>
    React.element =
    "Icon";
};

[@react.component]
let make =
    (
      ~icon,
      ~style: option(ReactDOMRe.Style.t)=?,
      ~theme: option([@bs.string] [ | `filled | `outlined | `twoTone])=?,
      ~spin: option(bool)=?,
      ~rotate: option(int)=?,
      ~component: option(React.element)=?,
      ~twoToneColor: option(string)=?,
    ) =>
  <AntIcon
    _type={Util.tts(iconToJs(icon))}
    style
    theme
    spin
    rotate
    component
    twoToneColor
  />;